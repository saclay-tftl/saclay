            <para PARENT="list-item"/>
            <ITEXT CH=". Il cherche à en­trer dans le CEA pour es­pion­ner"/>
            <para PARENT="list-item"/>
            <ITEXT CH=". Il com­mence le montage d'une machine à remonter le temps "/>
            <para PARENT="list-item"/>
            <ITEXT CH=". Il pra­tique des expériences de détournement de l'accélérateur de par­ti­cu­les"/>
            <para PARENT="list-item"/>
